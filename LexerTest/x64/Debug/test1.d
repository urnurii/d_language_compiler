import std.stdio;

void main() {
    // Integer types
    int a = 10;
    int b = -5;
    
    // Character type
    char c = 'A';
    char d = 'z';
    
    // String type
    string s1 = "Hello, D!";
    string s2 = "Test string";
    
    // Assignment operations
    a = 15;
    b += 3;
    
    writeln("Integer a: ", a);
    writeln("Integer b: ", b);
    writeln("Character c: ", c);
    writeln("Character d: ", d);
    writeln("String s1: ", s1);
    writeln("String s2: ", s2);
}